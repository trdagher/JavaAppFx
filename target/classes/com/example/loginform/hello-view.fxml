<?xml version="1.0" encoding="UTF-8"?>

<?import com.gluonhq.charm.glisten.control.Icon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="377.0" prefWidth="368.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.loginform.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField accessibleRole="COMBO_BOX" blendMode="MULTIPLY" promptText="text" style="-fx-background-color: green; -fx-font-family: Sans Serif;" text="hello">
      <font>
         <Font name="Kartika" size="31.0" />
      </font>
   </TextField>
   <Icon content="HELP_OUTLINE" prefHeight="44.0" prefWidth="328.0" style="-fx-blend-mode: DARKEN;" />
   <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: red; -fx-font-style: italic; -fx-text-alignment: center; -fx-font-family: Matura MT Script Capitals;" text="welcome tony">
      <font>
         <Font name="Baskerville Old Face" size="32.0" />
      </font>
   </Text>

    <Label fx:id="welcomeText" text="hello">
      <font>
         <Font name="Matura MT Script Capitals" size="36.0" />
      </font></Label>
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
